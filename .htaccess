<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteRule ^res/([0-9]+)/[A-Za-z-]+/?$       resources.php?id=$1
    RewriteRule ^user/([0-9]+)      user/profile.php?user-id=$1
    RewriteRule ^category/([0-9]+)/[A-Za-z-]+/?$    ?cat-id=$1
    RewriteRule ^thread/([0-9]+)/[A-Za-z-]+/?$    ?thread-id=$1

</IfModule>
